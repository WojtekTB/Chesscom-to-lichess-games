{
  "name": "Chess.com+",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Adds some simple functions to make chess.com more usable",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "permissions": [
    "activeTab",
    "https://lichess.org/*"
  ]
}
